{
    "name": "julidev/laravel-sso-keycloak",
    "description": "This packages allow you authenticate users with Keycloak Server.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Julidev\\LaravelSsoKeycloak\\": "src/"
        }
    },
    "keywords": [
        "laravel",
        "keycloak",
        "jwt",
        "authentication",
        "sso"
    ],
    "minimum-stability": "dev",
    "require": {
        "guzzlehttp/guzzle": "^6.5|^7.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Julidev\\LaravelSsoKeycloak\\SsoWebGuardServiceProvider"
            ],
            "aliases": {
                "KeycloakWeb": "Julidev\\LaravelSsoKeycloak\\Facades\\KeycloakWeb"
            }
        }
    }
}
